    </div>\r\n        </div>\r\n        <div className=\"col-sm-5 mx-auto\">\r\n          <UpdateProfile profile={userProfile} />\r\n        </div>\r\n        <br />\r\n      </div>\r\n    );\r\n  } else {\r\n    return <Spinner />;\r\n  }\r\n};\r\n\r\nexport default Profile;\r\n","C:\\Users\\thakare_v\\Desktop\\loan-management-react\\src\\Components\\layout\\Dashboard\\AdminDash.js",[],"C:\\Users\\thakare_v\\Desktop\\loan-management-react\\src\\Components\\auth\\Register.js",[],"C:\\Users\\thakare_v\\Desktop\\loan-management-react\\src\\Components\\auth\\AdminLogin.js",[],"C:\\Users\\thakare_v\\Desktop\\loan-management-react\\src\\Components\\layout\\Spinner.js",[],"C:\\Users\\thakare_v\\Desktop\\loan-management-react\\src\\Components\\Details\\Details.js",[],"C:\\Users\\thakare_v\\Desktop\\loan-management-react\\src\\Components\\Profile\\UpdateProfile.js",[],"C:\\Users\\thakare_v\\Desktop\\loan-management-react\\src\\Components\\Charts\\TenureChart.js",[],["105","106"],"C:\\Users\\thakare_v\\Desktop\\loan-management-react\\src\\Components\\Charts\\TransactionStatusChart.js",[],"C:\\Users\\thakare_v\\Desktop\\loan-management-react\\src\\Components\\Charts\\LoanChart.js",[],"C:\\Users\\thakare_v\\Desktop\\loan-management-react\\src\\Components\\Charts\\AdminChart.js",[],"C:\\Users\\thakare_v\\Desktop\\loan-management-react\\src\\Components\\auth\\ProtectedRouteAdmin.js",[],"C:\\Users\\thakare_v\\Desktop\\loan-management-react\\src\\Components\\auth\\ProtectedRouteUser.js",[],"C:\\Users\\thakare_v\\Desktop\\loan-management-react\\src\\Components\\auth\\UserLogin.js",[],{"ruleId":"107","replacedBy":"108"},{"ruleId":"109","replacedBy":"110"},{"ruleId":"111","severity":1,"message":"112","line":56,"column":15,"nodeType":"113","endLine":60,"endColumn":16},{"ruleId":"114","severity":1,"message":"115","line":32,"column":6,"nodeType":"116","endLine":32,"endColumn":8,"suggestions":"117"},{"ruleId":"114","severity":1,"message":"118","line":57,"column":6,"nodeType":"116","endLi